<p id="notice"><%= notice %></p>

<h1>Listing Decisions</h1>

<table>
  <thead>
    <tr>
      <th>Choice</th>
      <th>Rt</th>
      <th>Ec</th>
      <th>Ic</th>
      <th>Mp</th>
      <th>Pp</th>
      <th>Days</th>
      <th>Next step</th>
      <th>Step</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @decisions.each do |decision| %>
      <tr>
        <td><%= decision.choice %></td>
        <td><%= decision.rt %></td>
        <td><%= decision.ec %></td>
        <td><%= decision.ic %></td>
        <td><%= decision.mp %></td>
        <td><%= decision.pp %></td>
        <td><%= decision.days %></td>
        <td><%= decision.next_step %></td>
        <td><%= decision.step_id %></td>
        <td><%= link_to 'Show', decision %></td>
        <td><%= link_to 'Edit', edit_decision_path(decision) %></td>
        <td><%= link_to 'Destroy', decision, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Decision', new_decision_path %>
